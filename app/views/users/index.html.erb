<div id="page-wrapper" >
  <div id="page-inner">
    <div class="row">
      <div class="col-md-12">
        <div class="panel panel-default">
          <div class="panel-heading">
            <h2 class="page-header">
             <i class="fa fa-file-image-o"></i><%= t ".All_users" %>
            </h2>
          </div>
          <table class="table table-striped table-advance table-hover">
            <hr>
            <thead>
              <tr>
                <th><%= t ".name" %></th>
                <th><%= t ".email" %></th>
              </tr>
            </thead>
            <tbody>
              <% @users.each do |user| %>
                <tr>
                  <td><%= link_to user.full_name, user %> </td>
                  <td><%= user.email %></td>
                </tr>
              <% end %>
            </tbody>
          </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>